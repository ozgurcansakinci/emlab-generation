<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Bean post processor to persist created beans -->
	<bean class="agentspring.graphdb.PersistingBeanPostProcessor" />
	
	<!-- Variables used more than once in the configuration file can be centrally placed here -->

	<bean name="startCashBL" class="java.lang.String">
	  <constructor-arg value="20e9" />
	</bean>

	<!-- Decarbonization Model -->
	<bean id="decarbonizationModel" class="emlab.gen.domain.agent.DecarbonizationModel"
		p:name="Decarbonization Model" p:simulationLength="40"
		p:co2TradingImplemented="false" p:realRenewableDataImplemented="true"
		p:iterationSpeedFactor="3" p:iterationSpeedCriterion="0.005" p:longTermContractsImplemented="false"
		p:capDeviationCriterion="0.03" p:simpleCapacityMarketEnabled="true"
		p:deletionOldPPDPBidsAndCashFlowsEnabled="false" p:deletionAge="5"
		p:noPrivateIntermittentRESInvestment="false">
	</bean>
		
	<bean id="demandGrowthTrendNL" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/Kaveri/demandGrowthDE.csv"
		p:startingYear="0" p:delimiter="," p:variableName="demandDE-1" />

	<!-- Substances -->
	<bean id="coal" class="emlab.gen.domain.technology.Substance" p:name="Coal"
		p:quality="1" p:energyDensity="1000" p:co2Density="0.095" p:unit="GJ"  />
		
	<bean id="naturalgas" class="emlab.gen.domain.technology.Substance"
		p:name="Natural Gas" p:quality="1" p:energyDensity="1000"
			p:co2Density="0.05194" p:unit="GJ" />
		
	<bean id="biomass" class="emlab.gen.domain.technology.Substance"
		p:name="Biomass" p:quality="0.5" p:energyDensity="1000" p:co2Density="0" p:unit="GJ" />
		
	<bean id="uranium" class="emlab.gen.domain.technology.Substance"
		p:name="Uranium" p:quality="1" p:energyDensity="1000" p:co2Density="0" p:unit="GJ" />
		
	<bean id="electricity" class="emlab.gen.domain.technology.Substance"
		p:name="Electricity" p:quality="1" p:energyDensity="0" p:co2Density="0" p:unit="MWh_el" />
		
	<bean id="co2" class="emlab.gen.domain.technology.Substance" p:name="CO2"
		p:quality="1" p:energyDensity="0" p:co2Density="1" p:unit="tCO2" />

	<!-- Zones -->
	<bean id="nl" class="emlab.gen.domain.gis.Zone" p:name="Country A" />

	<!-- PowerGrid -->
	<bean id="beneluxNode" class="emlab.gen.domain.technology.PowerGridNode"
	p:capacityMultiplicationFactor="1.0">
		<property name="zone" ref="nl" />
	</bean>
	
	<bean id="photovoltaicNodeBL" class="emlab.gen.domain.technology.IntermittentResourceProfile" 
		p:filename="/data/RESData2010.csv" p:timeSeriesAreInDifferentColumns="true" p:lengthInHours="8760" p:delimiter=","
		p:variableName="nl_node_PV">
		<property name="intermittentTechnology" ref="photovoltaicPGT"></property>
		<property name="intermittentProductionNode" ref="beneluxNode"></property>
	</bean>
	
	<bean id="WindNodeBL" class="emlab.gen.domain.technology.IntermittentResourceProfile" 
		p:filename="/data/RESData2010.csv" p:timeSeriesAreInDifferentColumns="true" p:lengthInHours="8760" p:delimiter=","
		p:variableName="nl_node_OnW">
		<property name="intermittentTechnology" ref="windPGT"></property>
		<property name="intermittentProductionNode" ref="beneluxNode"></property>
	</bean>
	
	<bean id="WindoffNodeBL" class="emlab.gen.domain.technology.IntermittentResourceProfile"
		p:filename="/data/RESData2010.csv" p:timeSeriesAreInDifferentColumns="true" p:lengthInHours="8760" p:delimiter=","
		p:variableName="nl_node_OffW">
		<property name="intermittentTechnology" ref="windOffshorePGT"></property>
		<property name="intermittentProductionNode" ref="beneluxNode"></property>
	</bean>

	<!-- Yearly segment for the annual optimization -->
	
	<bean id="yearlySegment" class="emlab.gen.domain.market.electricity.YearlySegment"
		p:yearlySegmentLengthInHours="8760" p:yearlySegmentID="1" />
		
	<!-- Inelastic hourly demand for ESM for NL -->
	
	<bean id="InElasticDemandNL" class="emlab.gen.trend.HourlyCSVTimeSeries" 
		p:filename="/data/NodeDemandTimeSeriesData.csv" p:variableName="nl" p:timeSeriesAreInDifferentColumns="true" 
		p:lengthInHours="8760" p:delimiter="," />

	<!-- Elastic daily demand for ESM for NL-->
	
	<bean id="elasticDemandNL" class="emlab.gen.trend.HourlyCSVTimeSeries" 
		p:filename="/data/NodeDemandDailySeriesData.csv" p:variableName="nl" p:timeSeriesAreInDifferentColumns="true" 
		p:delimiter="," p:lengthInHours="365" p:elasticDemandShift="24"/>		
		
	<!-- Segments of load duration curves -->
	<bean id="segment1" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="20" />
	<bean id="segment2" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="19" />
	<bean id="segment3" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="18" />
	<bean id="segment4" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="17" />
	<bean id="segment5" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="16" />
	<bean id="segment6" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="15" />
	<bean id="segment7" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="14" />
	<bean id="segment8" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="13" />
	<bean id="segment9" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="12" />
	<bean id="segment10" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="11" />
	<bean id="segment11" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="10" />
	<bean id="segment12" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="9" />
	<bean id="segment13" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="8" />
	<bean id="segment14" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="7" />
	<bean id="segment15" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="6" />
	<bean id="segment16" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="5" />
	<bean id="segment17" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="4" />
	<bean id="segment18" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="3" />
	<bean id="segment19" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="2" />
	<bean id="segment20" class="emlab.gen.domain.market.electricity.Segment"
		p:segmentID="1" />
		
	<bean id="yearlySegmentLoadNL" class="emlab.gen.domain.market.electricity.YearlySegmentLoad">	
		<property name="hourlyInElasticBaseDemandForYearlySegment" ref="InElasticDemandNL" />
		<property name="dailyElasticBaseDemandForYearlySegment" ref="elasticDemandNL" />
		<property name="yearlySegment" ref="yearlySegment" />
		<property name="electricitySpotMarket" ref="beneluxElectricitySpotMarket" />
	</bean>

	<!-- Markets -->
	<!-- Electricity Spot -->
	<bean id="beneluxElectricitySpotMarket" class="emlab.gen.domain.market.electricity.ElectricitySpotMarket"
		p:name="Country A electricity spot market" p:auction="false" p:dailyDemandResponseImplemented="true"
		p:referencePrice="40" p:valueOfLostLoad="5000" p:lookback="5" p:backlookingForDemandForecastinginDismantling="5"
		p:storageImplemented="true">
		<property name="substance" ref="electricity" />
		<property name="demandGrowthTrend" ref="demandGrowthTrendNL" />
		<property name="zone" ref="nl" />
		<property name="yearlySegment" ref="yearlySegment" />
		<property name="yearlySegmentLoad" ref="yearlySegmentLoadNL" />
		<property name="loadDurationCurve">
			<set>
				<ref bean="segment1nl" />
				<ref bean="segment2nl" />
				<ref bean="segment3nl" />
				<ref bean="segment4nl" />
				<ref bean="segment5nl" />
				<ref bean="segment6nl" />
				<ref bean="segment7nl" />
				<ref bean="segment8nl" />
				<ref bean="segment9nl" />
				<ref bean="segment10nl" />
				<ref bean="segment11nl" />
				<ref bean="segment12nl" />
				<ref bean="segment13nl" />
				<ref bean="segment14nl" />
				<ref bean="segment15nl" />
				<ref bean="segment16nl" />
				<ref bean="segment17nl" />
				<ref bean="segment18nl" />
				<ref bean="segment19nl" />
				<ref bean="segment20nl" />
			</set>
		</property>
	</bean>
	

	<bean id="segment1nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment1" />
	</bean>
	<bean id="segment2nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment2" />
	</bean>
	<bean id="segment3nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment3" />
	</bean>
	<bean id="segment4nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment4" />
	</bean>
	<bean id="segment5nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment5" />
	</bean>
	<bean id="segment6nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment6" />
	</bean>
	<bean id="segment7nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment7" />
	</bean>
	<bean id="segment8nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment8" />
	</bean>
	<bean id="segment9nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment9" />
	</bean>
	<bean id="segment10nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment10" />
	</bean>
	<bean id="segment11nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment11" />
	</bean>
	<bean id="segment12nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment12" />
	</bean>
	<bean id="segment13nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment13" />
	</bean>
	<bean id="segment14nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment14" />
	</bean>
	<bean id="segment15nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment15" />
	</bean>
	<bean id="segment16nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment16" />
	</bean>
	<bean id="segment17nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment17" />
	</bean>
	<bean id="segment18nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment18" />
	</bean>
	<bean id="segment19nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment19" />
	</bean>
	<bean id="segment20nl" class="emlab.gen.domain.market.electricity.SegmentLoad">
		<property name="segment" ref="segment20" />
	</bean>

	<!-- Commodity Markets -->
	<bean id="coalMarket" class="emlab.gen.domain.market.CommodityMarket"
		p:auction="false" p:name="Coal market">
		<property name="substance" ref="coal" />
	</bean>

	<bean id="naturalgasMarket" class="emlab.gen.domain.market.CommodityMarket"
		p:auction="false" p:name="Natural gas market">
		<property name="substance" ref="naturalgas" />
	</bean>

	<bean id="biomassMarket" class="emlab.gen.domain.market.CommodityMarket"
		p:auction="false" p:name="Biomass market">
		<property name="substance" ref="biomass" />
	</bean>

	<bean id="nuclearMarket" class="emlab.gen.domain.market.CommodityMarket"
		p:auction="false" p:name="Uranium market">
		<property name="substance" ref="uranium" />
	</bean>

	<!-- CO2 Auction -->
	<bean id="co2Auction" class="emlab.gen.domain.market.CO2Auction"
		p:auction="true" p:referencePrice="0" p:name="CO2 auction"
		p:substance-ref="co2" />

	<!-- Fuel Prices -->
		
	<bean id="coalPrice" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/Kaveri/coalPrice.csv" p:startingYear="0"
		p:delimiter="," p:variableName="coalPrice-2" />
	<bean id="gasPrice" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/Kaveri/gasPrice.csv" p:startingYear="0"
		p:delimiter="," p:variableName="gasPrice-2" />
	<bean id="biomassPrice" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/Kaveri/biomassPrice.csv"
		p:startingYear="0" p:delimiter="," p:variableName="biomassPrice-2" />
	<bean id="uraniumPrice" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/Kaveri/uraniumPrice.csv"
		p:startingYear="0" p:delimiter="," p:variableName="uraniumPrice-2" />

	<!-- Agents -->
	<bean id="coalSupplier" class="emlab.gen.domain.agent.CommoditySupplier">
		<property name="name" value="Coal supplier" />
		<property name="substance" ref="coal" />
		<property name="priceOfCommodity" ref="coalPrice" />
	</bean>

	<bean id="gasSupplier" class="emlab.gen.domain.agent.CommoditySupplier">
		<property name="name" value="Gas supplier" />
		<property name="substance" ref="naturalgas" />
		<property name="priceOfCommodity" ref="gasPrice" />
	</bean>

	<bean id="biomassSupplier" class="emlab.gen.domain.agent.CommoditySupplier">
		<property name="name" value="Biomass supplier" />
		<property name="substance" ref="biomass" />
		<property name="priceOfCommodity" ref="biomassPrice" />
	</bean>

	<bean id="uraniumSupplier" class="emlab.gen.domain.agent.CommoditySupplier">
		<property name="name" value="Uranium supplier" />
		<property name="substance" ref="uranium" />
		<property name="priceOfCommodity" ref="uraniumPrice" />
	</bean>

	<bean id="energyConsumer" class="emlab.gen.domain.agent.EnergyConsumer"
		p:ltcMaximumCoverageFraction="0.8" p:contractDurationPreferenceFactor=".03"
		p:contractWillingnessToPayFactor="1.2">
		<property name="name" value="Energy consumer" />
	</bean>

		<bean id="energyProducerA" class="emlab.gen.domain.agent.EnergyProducer"
		p:name="Energy Producer A" p:cash="6e9"
		p:investmentRole-ref="standardInvestmentRole" p:investmentRequired="false"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="7" p:pastTimeHorizon="5"
		p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7" p:simpleCapacityMarketEnabled="true"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="false" p:priceMarkUp="1" 
		p:numberOfYearsBacklookingForForecasting="5"
		p:investorMarket-ref="beneluxElectricitySpotMarket"/>
		
	<bean id="energyProducerB" class="emlab.gen.domain.agent.EnergyProducer"
		p:name="Energy Producer B" p:cash="6e9"
		p:investmentRole-ref="standardInvestmentRole" p:investmentRequired="true"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="7" p:pastTimeHorizon="5"
		p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7" p:simpleCapacityMarketEnabled="true"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="1" 
		p:numberOfYearsBacklookingForForecasting="5"
		p:investorMarket-ref="beneluxElectricitySpotMarket"/>
		
	<bean id="energyProducerC" class="emlab.gen.domain.agent.EnergyProducer"
		p:name="Energy Producer C" p:cash="6e9"
		p:investmentRole-ref="standardInvestmentRole" p:investmentRequired="true"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="7" p:pastTimeHorizon="5"
		p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7" p:simpleCapacityMarketEnabled="true"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="1" 
		p:numberOfYearsBacklookingForForecasting="5"
		p:investorMarket-ref="beneluxElectricitySpotMarket"/>
		
	<bean id="energyProducerD" class="emlab.gen.domain.agent.EnergyProducer"
		p:name="Energy Producer D" p:cash="6e9"
		p:investmentRole-ref="standardInvestmentRole" p:investmentRequired="true"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="7" p:pastTimeHorizon="5"
		p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7" p:simpleCapacityMarketEnabled="true"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="1" 
		p:numberOfYearsBacklookingForForecasting="5"
		p:investorMarket-ref="beneluxElectricitySpotMarket"/>
		
	<bean id="energyProducerE" class="emlab.gen.domain.agent.EnergyProducer"
		p:name="Energy Producer E" p:cash="6e9"
		p:investmentRole-ref="standardInvestmentRole" p:investmentRequired="true"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="7" p:pastTimeHorizon="5"
		p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7" p:simpleCapacityMarketEnabled="true"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="1" 
		p:numberOfYearsBacklookingForForecasting="5"
		p:investorMarket-ref="beneluxElectricitySpotMarket"/>
	
<bean id="standardInvestmentRole" class="emlab.gen.role.investment.InvestInPowerGenerationTechnologiesAnnual"/>

<bean id="targetInvestmentRole" class="emlab.gen.role.investment.TargetInvestmentRole"/>

	<bean id="StakeholderA" class="emlab.gen.domain.agent.Stakeholder" p:name="Stakeholder A">
		<property name="energyProducerForPaymentOfDividends" ref="energyProducerA" />
	</bean>
	
	<bean id="StakeholderB" class="emlab.gen.domain.agent.Stakeholder" p:name="Stakeholder B">
		<property name="energyProducerForPaymentOfDividends" ref="energyProducerB" />
	</bean>
	
	<bean id="StakeholderC" class="emlab.gen.domain.agent.Stakeholder" p:name="Stakeholder C">
		<property name="energyProducerForPaymentOfDividends" ref="energyProducerC" />
	</bean>
	
	<bean id="StakeholderD" class="emlab.gen.domain.agent.Stakeholder" p:name="Stakeholder D">
		<property name="energyProducerForPaymentOfDividends" ref="energyProducerD" />
	</bean>
	
	<bean id="StakeholderE" class="emlab.gen.domain.agent.Stakeholder" p:name="Stakeholder E">
		<property name="energyProducerForPaymentOfDividends" ref="energyProducerE" />
	</bean>
	
	<bean id="regulatorA" class="emlab.gen.domain.agent.Regulator"
		p:name="Regulator A" p:capacityMarketPriceCap="58940"
		p:reserveMargin="0.156" p:targetPeriod="0"
		p:reserveDemandLowerMargin="0.025" p:reserveDemandUpperMargin="0.025"
		p:numberOfYearsLookingBackToForecastDemand="4"> 
		<property name="zone" ref="nl" />
	</bean>
		
	<bean id="capacityMarketA" class="emlab.gen.domain.market.capacity.CapacityMarket"
		p:name="Capacity Market A" p:storageBiddingAllowed="true" p:renewableTargetInvestorCanInvest="true"
		p:baseCapacityMarketDemand="75000">
		<property name="regulator" ref="regulatorA"/>
		<property name="consumer" ref="energyConsumer"/>
		<property name="zone" ref="nl" />
	</bean>
	
	<bean id="capitalCostsforStoragePerMWH" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/Kaveri/CapitalCosts.csv" p:startingYear="0"
		p:delimiter="," p:variableName="CapitalCost-1" />
		
	<bean id="operationsCostsforStoragePerMWH" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/Kaveri/OandMCosts.csv" p:startingYear="0"
		p:delimiter="," p:variableName="OandMCost-1" />
		
	<bean id="inflowEfficiency" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/Kaveri/IFEff.csv" p:startingYear="0"
		p:delimiter="," p:variableName="IFEff-1" />
		
	<bean id="outflowEfficiency" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/Kaveri/OFEff.csv" p:startingYear="0"
		p:delimiter="," p:variableName="OFEff-1" />
		
	<!-- Storage Energy technologies -->
	
	<bean id="storageUnitForNL" class="emlab.gen.domain.technology.EnergyStorageTechnology"
		p:name="Storage Unit for NL ESM" p:baseMaxStorageCapacity="20000" p:baseMaxStorageChargingRate="5000" 
		p:baseMaxStorageDischargingRate="5000" p:initialStateOfChargeInStorage="10000" p:finalStateOfChargeInStorage="10000" >
		<property name="ownerStorageUnit" ref="energyProducerA"/>
		<property name="fixedCapitalCostTimeSeriesForStoragePerMWh" ref="capitalCostsforStoragePerMWH"/>
		<property name="fixedOperationAndMaintainanceCostTimeSeriesForStoragePerMWh" ref="operationsCostsforStoragePerMWH" />
		<property name="efficiencyInFlowTimeSeries" ref="inflowEfficiency" />
		<property name="efficiencyOutFlowTimeSeries" ref="outflowEfficiency" />
	</bean>

